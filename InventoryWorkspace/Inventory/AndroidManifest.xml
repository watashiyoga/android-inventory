        </activity>
        <activity android:name=".ShowProduct" android:label="@string/app_name"></activity>
		<activity android:name=".EditProduct" android:label="@string/app_name"></activity>
		<activity android:label="@string/app_name" android:name=".ListProduct"></activity>
		<activity android:name=".ShowFullScreenPicture" android:label="@string/app_name"></activity>
		<activity android:name="EditType" android:label="@string/app_name"></activity>
